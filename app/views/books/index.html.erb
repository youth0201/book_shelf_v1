<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>書籍名</th>
      <th>価格</th>
      <th>出版社</th>
      <th>刊行日</th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.id %></td>
        <td><%= book.name %></td>
        <td><%= book.price %></td>
        <td><%= book.publish %></td>
        <td><%= book.publish_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>